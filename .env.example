# Environment Configuration
# Copy this file to .env and fill in your actual values

# ========================================
# GROQ API Configuration (REQUIRED)
# ========================================
# Get your API key from: https://console.groq.com/keys
EXPO_PUBLIC_GROQ_API_KEY=your_groq_api_key_here

# ========================================
# University Configuration
# ========================================
EXPO_PUBLIC_UNIVERSITY_DOMAIN=university.edu
EXPO_PUBLIC_UNIVERSITY_NAME=Your University Name

# ========================================
# Optional: Firebase (for analytics & logging)
# ========================================
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# ========================================
# Optional: Backend API (if you have one)
# ========================================
EXPO_PUBLIC_API_URL=https://your-backend-api.com/api
